<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>LoginManagement</servlet-name>
        <servlet-class>Servlet.LogInAndOutServlet.LoginManagement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginManagement</servlet-name>
        <url-pattern>/mainPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutMangement</servlet-name>
        <servlet-class>Servlet.LogInAndOutServlet.LogoutMangement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutMangement</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QRcodeServlet</servlet-name>
        <servlet-class>Servlet.QRcode.QRcodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QRcodeServlet</servlet-name>
        <url-pattern>/QRcodePage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CollegeServlet</servlet-name>
        <servlet-class>Servlet.basicDatasServlet.CollegeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CollegeServlet</servlet-name>
        <url-pattern>/CollegePage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>majorServlet</servlet-name>
        <servlet-class>Servlet.basicDatasServlet.majorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>majorServlet</servlet-name>
        <url-pattern>/majorPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>classServlet</servlet-name>
        <servlet-class>Servlet.basicDatasServlet.classServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>classServlet</servlet-name>
        <url-pattern>/classPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>studentsServlet</servlet-name>
        <servlet-class>Servlet.basicDatasServlet.studentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentsServlet</servlet-name>
        <url-pattern>/studentsPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>teachersServlet</servlet-name>
        <servlet-class>Servlet.basicDatasServlet.teachersServlet</servlet-class>
