    </servlet>
    <servlet-mapping>
        <servlet-name>FillTableServlet</servlet-name>
        <url-pattern>/HealthCode</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showDatasServlet</servlet-name>
        <servlet-class>Servlet.showDatas.showDatasServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showDatasServlet</servlet-name>
        <url-pattern>/showDatasPage</url-pattern>
    </servlet-mapping>

</web-app>